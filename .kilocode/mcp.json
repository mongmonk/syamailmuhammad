{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      },
      "alwaysAllow": [
        "resolve-library-id",
        "get-library-docs"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:/laragon/www/syamail"
      ],
      "alwaysAllow": [
        "read_file",
        "read_text_file",
        "read_media_file",
        "read_multiple_files"
      ]
    },
    "sequentialthinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "alwaysAllow": [
        "sequentialthinking"
      ]
    },
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "alwaysAllow": [
        "create_entities",
        "create_relations",
        "add_observations",
        "delete_entities"
      ]
    },
    "laravel-boost": {
      "command": "php",
      "args": [
        "artisan",
        "boost:mcp"
      ],
      "alwaysAllow": [
        "application-info",
        "search-docs",
        "read-log-entries",
        "browser-logs",
        "database-connections",
        "database-query",
        "database-schema",
        "get-absolute-url",
        "get-config",
        "last-error",
        "list-artisan-commands",
        "list-available-config-keys",
        "list-available-env-vars",
        "list-routes",
        "report-feedback",
        "tinker"
      ]
    }
  }
}